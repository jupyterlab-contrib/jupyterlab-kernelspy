{
  "name": "jupyterlab-kernelspy",
  "version": "3.0.1",
  "description": "A Jupyter Lab extension for inspecting messages to/from a kernel",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/vidartf/jupyterlab-kernelspy",
  "bugs": {
    "url": "https://github.com/vidartf/jupyterlab-kernelspy/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vidartf/jupyterlab-kernelspy.git"
  },
  "license": "BSD-3-Clause",
  "author": "Vidar Tonaas Fauske",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/plugin.js",
  "types": "lib/plugin.d.ts",
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf jupyterlab-kernelspy/static",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "install:extension": "jupyter labextension develop --overwrite .",
    "prepack": "jlpm run clean && jlpm run build",
    "update:all": "update-dependency --minimal --regex .*",
    "update:lab": "update-dependency --minimal --regex ^@jupyterlab/",
    "update:lab:next": "update-dependency --minimal --regex ^@jupyterlab/ ^next",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.0-beta.6",
    "@jupyterlab/apputils": "^3.0.0-beta.6",
    "@jupyterlab/mainmenu": "^3.0.0-beta.6",
    "@jupyterlab/notebook": "^3.0.0-beta.6",
    "@jupyterlab/ui-components": "^3.0.0-beta.6",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/disposable": "^1.4.3",
    "@lumino/properties": "^1.2.3",
    "@lumino/widgets": "^1.14.0",
    "react-inspector": "^5.0.1"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^3.0.0-beta.6",
    "@jupyterlab/buildutils": "^3.0.0-beta.6",
    "@jupyterlab/docregistry": "^3.0.0-beta.6",
    "@jupyterlab/services": "^6.0.0-beta.6",
    "@lumino/commands": "^1.11.3",
    "@lumino/messaging": "^1.4.3",
    "@types/react-inspector": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "~3.9.0"
  },
  "jupyterlab": {
    "extension": "lib/plugin",
    "outputDir": "jupyterlab-kernelspy/static"
  }
}
